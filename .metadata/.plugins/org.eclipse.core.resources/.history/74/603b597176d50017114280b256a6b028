		{
			for(int col = 0; col < numCols; col++)
			{
				map[]
			}
		}
		
		
		
	}
	
	public static boolean inBounds(int row, int col, int numRows, int numCols)
	{
		return (row >= 0) && (col >= 0) && (row< numRows) && (col < numCols);
	}
}
